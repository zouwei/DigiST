{
    "extends": "standard",
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": "off",
        "semi": ["error", "always"],
        "eqeqeq": "off",
        "no-unused-vars": "warn",
        "camelcase": "off"
    },
    "parserOptions": { "ecmaVersion": 8 } 
}